[Panel]
车辆限行Panel = script-name=xianxing, update-interval=3600

[Script]
# Panel
IThome = type=http-response, script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/ithome/ithome.cookie.js, timeout=60
# Cron
IThomeCron = type=cron,cronexp="10 7 * * *",script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/ithome/ithome.js,timeout=100,script-update-interval=3600,argument=city={{{city}}}&loo={{{loo}}}&cartype={{{cartype}}}


[MITM]
hostname = napi.ithome.com